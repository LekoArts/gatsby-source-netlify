{
  "name": "gatsby-source-netlify",
  "version": "2.0.0",
  "main": "index.js",
  "repository": "git@github.com:LekoArts/gatsby-source-netlify.git",
  "author": "LekoArts <lekoarts@gmail.com>",
  "license": "MIT",
  "keywords": [
    "netlify",
    "gatsby-plugin",
    "gatsby-source"
  ],
  "peerDependencies": {
    "gatsby": ">=3.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "lint": "eslint . --ignore-path .gitignore --ext .js",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write README.md"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "netlify": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby-package": "^1.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.3.2"
  }
}
